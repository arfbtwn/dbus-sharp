CONFIG = @CONFIGURATION@
TARGETPATH = bin/$(CONFIG)/$(TARGET)
XBUILD_Q = $(XBUILD) /p:Configuration=$(CONFIG) /nologo /verbosity:quiet

all: $(TARGETPATH)

$(TARGETPATH): $(CSFILES)
	$(XBUILD_Q) /target:Build $(ASSEMBLY).csproj

clean:
	$(XBUILD_Q) /target:Clean $(ASSEMBLY).csproj

# This is not needed on that branch
# it's for compat with wrench
setup: ;
run: check

EXTRA_DIST = \
	$(CSFILES) \
	$(ASSEMBLY).csproj

CLEANFILES = \
	$(TARGETPATH) \
	$(TARGETPATH).mdb

MAINTAINERCLEANFILES = \
	Makefile.in
